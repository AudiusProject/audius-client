{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["preact", "standard-jsx"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "space-before-function-paren": "off"
  }
}
