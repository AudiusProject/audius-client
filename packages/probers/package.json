{
  "name": "probers",
  "version": "0.0.1",
  "description": "Automated client tests",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:open-stage": "cypress open --env configFile=stage",
    "cypress:run": "cypress run",
    "cypress:run-stage": "cypress run --env configFile=stage",
    "test:target": "env-cmd -f .env jest --runInBand",
    "test": "env-cmd -f .env jest ../__tests__ --runInBand",
    "test:ci": "env-cmd -f .env.ci jest ../__tests__ --runInBand",
    "pings": "jest ../__ping__ --runInBand",
    "lint": "./node_modules/.bin/standard",
    "lint:fix": "./node_modules/.bin/standard --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.10.7",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@testing-library/cypress": "^8.0.2",
    "@types/fs-extra": "^9.0.13",
    "babel-jest": "^24.8.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cypress": "^9.3.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-wait-until": "^1.7.2",
    "env-cmd": "^9.0.3",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "puppeteer": "^1.17.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "typescript": "^4.5.5"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  }
}
